{
  "name": "electron-starter",
  "version": "1.0.0",
  "description": "a electron starter",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "start-electron": "cross-env NODE_ENV=development electron .",
    "dev": "cross-env START_HOT=1 NODE_ENV=development webpack-dev-server --config ./config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "dist": "npm run build & npm run electron-builder",
    "dist-win": "npm run build & npm run electron-builder --win -ia32",
    "dist-all": "npm run build & npm run electron-builder -mwl",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
    "clean": "rm -rf ./dist ./render/dist",
    "flow": "flow"
  },
  "author": "Cody1988",
  "homepage": "https://github.com/Cody1988/electron-react-starter",
  "license": "Apache License 2.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "electron": "^2.0.0",
    "electron-builder": "^20.13.2",
    "electron-devtools-installer": "^2.2.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.71.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "build": {
    "appId": "com.cody.electron.starter",
    "dmg": {},
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "antd": "^3.5.0",
    "dva": "^2.2.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
}
